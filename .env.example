# Copy this file to `.env.local` and replace each placeholder with a secret from
# your deployment platform or local credentials vault. Do not commit filled-in files.

# Client configuration
NEXT_PUBLIC_DEFAULT_COUNTRY=IT
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_SUPABASE_PROJECT.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
NEXT_PUBLIC_MAPBOX_TOKEN=YOUR_MAPBOX_TOKEN
NEXT_PUBLIC_UNSPLASH_KEY=YOUR_UNSPLASH_ACCESS_KEY
NEXT_PUBLIC_ASSEMBLYAI_API_KEY=YOUR_PUBLIC_ASSEMBLYAI_KEY
# NEXT_PUBLIC_MAPTILER_API_KEY=your_maptiler_api_key_here
# NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=false

# Server-side secrets (never expose via NEXT_PUBLIC_*)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
PIXABAY_API_KEY=YOUR_PIXABAY_API_KEY
GOOGLE_PLACES_API_KEY=YOUR_GOOGLE_PLACES_API_KEY
OPENAI_API_KEY=YOUR_OPENAI_API_KEY
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
YOUTUBE_API_KEY=YOUR_YOUTUBE_API_KEY
FIRECRAWL_API_KEY=YOUR_FIRECRAWL_API_KEY
ASSEMBLYAI_API_KEY=YOUR_ASSEMBLYAI_API_KEY
ELEVENLABS_API_KEY=YOUR_ELEVENLABS_API_KEY
# GOOGLE_CUSTOM_SEARCH_KEY=your_google_custom_search_key_here
# REDDIT_CLIENT_ID=your_reddit_client_id_here
# REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# Repository metadata (optional)
GITHUB_USER=your-github-username
GITHUB_EMAIL=your-email@example.com
GITHUB_REPO=trip

# Optional map debugging toggle
# NEXT_PUBLIC_DEBUG_MAP=false
